      </ldap:rdn-sequence>
    </adm:profile>
    <adm:profile name="cli">
      <cli:relation>
        <cli:default-property name="password-attribute" />
        <cli:default-property name="default-password-storage-scheme" />
      </cli:relation>
    </adm:profile>
  </adm:relation>
  <adm:relation name="log-publisher">
